{
  "name": "idworker",
  "version": "1.0.1",
  "description": "高效GUID产生算法(sequence),基于Snowflake实现64位自增ID算法",
  "main": "dist/index.js",
  "dependencies": {
    "default-gateway": "^6.0.3"
  },
  "devDependencies": {
    "@types/default-gateway": "^3.0.1",
    "@types/jest": "^28.1.7",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.4",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "prepack": "husky install",
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "tsc"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/god-pig/idworker-node.git"
  },
  "keywords": [
    "snowflake"
  ],
  "author": "litao",
  "license": "MIT"
}
